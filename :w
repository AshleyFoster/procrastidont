require 'rails_helper'

RSpec.feature "Login" do
  scenario "register redirects user to home" do
    visit '/'
    click_on 'Login'

    fill_in 'Email', with: 'user@example.com'
    fill_in 'Password', with: 'caplin'

    click_button 'Log in'

    expect(page).to have_content 'Logged in as'
  end
end
